{
  "parser": "babel-eslint",
  "rules": {
    "strict": 2,
    "no-undef":2,
    "no-unused-vars": [2, {"vars": "local"}],
     "indent": [
            "error",
            4,
            { "SwitchCase": 1 }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
  }
}